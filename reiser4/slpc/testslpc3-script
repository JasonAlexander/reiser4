#!/bin/sh

TESTS="16 32 64 128 250 500 750 1000 1500 2000 2500 3000 3500 4000 4500 5000 5500 6000 6500 7000 7500 8000 9000 10000 11000 12000 13000 14000 15000 16000 18000 20000 22000 24000 26000 28000 30000 32000 48000 64000 128000 256000 512000 1024000"
NREPS=100000

for t in $TESTS; do
  ./testslpc3 $t $NREPS
  ./testslpc3 $t $NREPS
  ./testslpc3 $t $NREPS
done
