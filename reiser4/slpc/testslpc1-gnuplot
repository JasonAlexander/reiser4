#!/usr/local/bin/gnuplot -persist


set terminal gif
set grid
set log x

set xlabel "Node Size"
set ylabel "Avg. Cycles"
set data style lines

set title  "Skip List Insertion (1M keys)"
set output 'slpc_insert.gif'
plot 'slpc.out' using 1:2 title "Read-write locks", 'slpc.out' using 1:5 title "Exclusive locks", 'slpc.out' using 1:8 title "No locks"

set title  "Skip List Search (1M keys)"
set output 'slpc_search.gif'
plot 'slpc.out' using 1:3 title "Read-write locks", 'slpc.out' using 1:6 title "Exclusive locks", 'slpc.out' using 1:9 title "No locks"

set title  "Skip List Deletion (1M keys)"
set output 'slpc_delete.gif'
plot 'slpc.out' using 1:4 title "Read-write locks", 'slpc.out' using 1:7 title "Exclusive locks", 'slpc.out' using 1:10 title "No locks"
